{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "DefaultTopic": "test",
    "GroupId": "demo-consumer-group",
    "EnableIdempotence": true,
    "Acks": "all",
    "Compression": "snappy",
    "LingerMs": 20,
    "BatchSizeBytes": 65536,
    "AutoOffsetReset": "Earliest",
    "ConsumeBufferCapacity": 1000
  }
}