{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "Memory": {
      "SizeLimit": 5000
    },
    "Distributed": {
      "RedisConnection": "redis:6379",
      "InstanceName": "shopcache:"
    },
    "Defaults": {
      "ProductTtlSeconds": 120,
      "MemoryTtlSeconds": 60
    }
  }
}