{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "Connection": "redis:6379,abortConnect=false"
  },
  "Cache": {
    "Memory": {
      "SizeLimit": 5000
    },
    "Distributed": {
      "RedisConnection": "redis:6379",
      "InstanceName": "shopcache:",
      "ProductTtlSeconds": 120,
      "NullTtlSeconds": 15,
      "EarlyRefreshSeconds": 5,
      "EnableNegativeCaching": true,
      "EnableCompression": true,
      "KeyPrefix": "dist:product:v1:"
    },
    "Defaults": {
      "ProductTtlSeconds": 120,
      "MemoryTtlSeconds": 60,
      "NullTtlSeconds": 10,
      "EarlyRefreshSeconds": 5,
      "EnableNegativeCaching": true,
      "EntrySize": 1
    },
    "Lazy": {
      "ProductTtlSeconds": 120,
      "NullTtlSeconds": 15,
      "EnableNegativeCaching": true,
      "KeyPrefix": "lazy:product:v1:"
    }
  }
}